<?xml version="1.0" encoding="UTF-8"?>
<reference xml:id="fn_unparsed_entity" scheme="xsl">
   <body>
      <purpose><p>Two functions that access the public and system identifiers of
         parsed entities in the DTD of a source document.</p></purpose>
      <syntax>unparsed-entity-public-id (name)</syntax>
      <properties type="parameter">
         <property xml:id="name">
            <name>name</name>
            <description>The XML Name of the entity.</description>
            <type>xs:string</type>
            <required state="required"/>
         </property>
         
      </properties>
      <related label="related">
         <link href="xsl_functions"/>
      </related>
   </body>
</reference>
